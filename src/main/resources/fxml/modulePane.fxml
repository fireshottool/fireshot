<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import java.lang.String?>
<VBox xmlns:fx="http://javafx.com/fxml/1" minWidth="200" prefWidth="232.0"
      xmlns="http://javafx.com/javafx/8.0.181"
      spacing="30"
      fx:controller="me.fox.ui.settings.controller.pane.ModulePaneController">

    <ListView fx:id="listView" styleClass="listView">
        <String fx:value="Appearance"/>
        <String fx:value="Draw"/>
        <String fx:value="Screenshot"/>
        <String fx:value="Hotkeys"/>
    </ListView>
</VBox>
